{
  "env": {
    "es6": true,
    "node": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb", "prettier", "plugin:import/recommended"],
  "plugins": ["import"],
  "root": true,
  "rules": {
    "no-bitwise": 0,
    "semi": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "no-shadow": 0,
    "no-plusplus": 0,
    "no-unused-vars": ["warn", { "varsIgnorePattern": "ignore" }],
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-debugger": 1,
    "no-restricted-exports": 0,
    "no-use-before-define": 0,
    "no-return-assign": 0,
    "import/extensions": 0,
    "import/no-cycle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0,
    "import/no-commonjs": 2
  }
}
